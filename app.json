{
  "expo": {
    "name": "Caculadora",
    "slug": "Calculadora",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconcalculadora.png",
    "splash": {
      "image": "./assets/splashcalculator.png",
      "resizeMode": "contain",
      "backgroundColor": "#38b6ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#38b6ff"
    },
    "androidNavigationBar":{
      "backgroundColor": "#FFF"
    }
  }
}
